<main>
  <h1 class="visually-hidden">Bashboard</h1>

  <!-- Username, Password and Profile -->
  <div class="container px-4 py-4">
    <h2 class="pb-2 border-bottom">Dashboard</h2>
    <div class="row g-4 py-5 row-cols-1 row-cols-lg-3">
      <div class="col d-flex align-items-center">
        <div class="icon-square text-body-emphasis bg-body-secondary d-inline-flex align-items-center justify-content-center fs-4 flex-shrink-0 me-3">
          <svg class="bi" width="1em" height="1em"><use xlink:href="#user"/></svg>
        </div>
        <div>
          <a href="#" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#user-change">Change username</a>
        </div>
      </div>
      <div class="col d-flex align-items-center">
        <div class="icon-square text-body-emphasis bg-body-secondary d-inline-flex align-items-center justify-content-center fs-4 flex-shrink-0 me-3">
          <svg class="bi" width="1em" height="1em"><use xlink:href="#key"/></svg>
        </div>
        <div>
          <a href="#" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#pswd-change">Change password</a>
        </div>
      </div>
      <div class="col d-flex align-items-center">
        <div class="icon-square text-body-emphasis bg-body-secondary d-inline-flex align-items-center justify-content-center fs-4 flex-shrink-0 me-3">
          <svg class="bi" width="1em" height="1em"><use xlink:href="#profile"/></svg>
        </div>
        <div>
          <a href="#" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#profile-modal">My profile</a>
        </div>
      </div>

    </div>
  </div>

  <div class="container">
    <div class="border-bottom mx-4"></div>
  </div>

  <!-- DB Restore and Backup-->
  <div class="container px-4 py-4">
    <div class="row g-4 py-5 row-cols-1 row-cols-lg-3">
